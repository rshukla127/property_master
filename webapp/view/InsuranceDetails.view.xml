<mvc:View controllerName="com.public.storage.pao.controller.InsuranceDetails"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <ScrollContainer height="100%" width="100%" vertical="true" focusable="true">



        <VBox class="sapUiSmallMargin">
            <f:SimpleForm id="basicDetails" editable="true" layout="ResponsiveGridLayout" title="{i18n>insDetails} (Plant - {plantsModel>/plant} &amp; Property - {plantBasicDetailsModel>/LegacyPropertyNumber} - {plantsModel>/address})" labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2" singleContainerFullSize="false">
                <f:content>
                    <core:Title text="" visible="false"/>
                    <Label text="{i18n>finrepnum1}" />
                    <Input id="finrep1" value="{plantBasicDetailsModel>/FinrepNum1}" />
                    <Label text="{i18n>finrepnum2}" />
                    <Input id="finrep2" value="{plantBasicDetailsModel>/FinerepNum2}" />

                    <Label required="true" text="{i18n>insPrem2}" />
                    <Input id="insprem2" valueStateText="Value cannot be blank" valueState="{/InsPrem2000}" value="{plantBasicDetailsModel>/InsPrem2000}" />

                    <Label required="true" text="{i18n>insPrem3}" />
                    <Input id="insprem3" valueStateText="Value cannot be blank" valueState="{/InsPrem3000}" value="{plantBasicDetailsModel>/InsPrem3000}" />

                    <Label required="true" text="{i18n>insPrem4}" />
                    <Input id="insprem4" valueStateText="Value cannot be blank" valueState="{/InsPrem4000}" value="{plantBasicDetailsModel>/InsPrem4000}" />

                    <Label required="true" text="{i18n>insPrem5}" />
                    <Input id="insprem5" valueStateText="Value cannot be blank" valueState="{/InsPrem5000}" value="{plantBasicDetailsModel>/InsPrem5000}" />

                    <Label text="{i18n>finrepnum7}" />
                    <Input id="finrep7" value="{plantBasicDetailsModel>/FinrepNum7}" />

                    <Label text="{i18n>finrepnum8}" />
                    <Input id="finrep8" value="{plantBasicDetailsModel>/FinrepNum8}" />

                    <Label text="{i18n>finrepnum9}" />
                    <Input id="finrep9" value="{plantBasicDetailsModel>/FinrepNum9}" />
                    <Label required="true" text="{i18n>eRentalMax}" />
                    <Input id="eRental" valueStateText="Value cannot be blank" valueState="{/ErentalMaxDays}" value="{plantBasicDetailsModel>/ErentalMaxDays}" />

                    <core:Title text="" />

                    <Label required="true" valueStateText="Value cannot be blank" valueState="{/CddStartDate}" text="{i18n>cddStartDate}" />
                    <DatePicker id="startdate" placeholder="Choose Date.." value="{ 
                        path: 'plantBasicDetailsModel>/CddStartDate',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" displayFormat="yyyy-MM-dd" />

                    <Label text="{i18n>finrepD3}" />
                    <DatePicker id="finrepD3" placeholder="Choose Date.." value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate3',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd"/>

                    <Label text="{i18n>finrepD4}" />
                    <DatePicker id="finrepD4" placeholder="Choose Date.." value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate4',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" />
                    <Label text="{i18n>finrepD5}" />
                    <DatePicker id="finrepD5" placeholder="Choose Date.." value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate5',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd"/>

                    <Label text="{i18n>finrepD6}" />
                    <DatePicker id="finrepD6" placeholder="Choose Date.." value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate6',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" />

                    <Label text="{i18n>finrepD7}" />
                    <DatePicker id="finrepD7" placeholder="Choose Date.." value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate7',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" />
                    <Label text="{i18n>finrepD8}" />
                    <DatePicker id="finrepD8" value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate8',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" placeholder="Choose Date.."/>
                    <Label text="{i18n>finrepD9}" />
                    <DatePicker id="finrepD9" value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate9',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" placeholder="Choose Date.." />
                    <Label text="{i18n>finrepD10}" />
                    <DatePicker id="finrepD10" value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate10',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" placeholder="Choose Date.."/>

                </f:content>
            </f:SimpleForm>
            <VBox alignContent="End" alignItems="End">
                <Button text="Save and Continue" enabled="true" type="Emphasized" press="onPressSaveInsuranceDetails"></Button>
            </VBox>
        </VBox>


    </ScrollContainer>
</mvc:View>