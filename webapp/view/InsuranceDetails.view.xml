<mvc:View xmlns:mvc="sap.ui.core.mvc"
controllerName="com.public.storage.pao.controller.InsuranceDetails"
	xmlns="sap.uxap"
	xmlns:m="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" height="100%">

	<ObjectPageLayout id="ObjectPageLayout" upperCaseAnchorBar="false">
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<expandedHeading>
					<m:Title text="Insurance Details" wrapping="true" />
				</expandedHeading>

				<snappedHeading>
					<m:HBox>
						<m:VBox>
							<m:Title text="Insurance Details" wrapping="true"/>
							<m:Label text="Fill your insurance details"/>
						</m:VBox>
					</m:HBox>
				</snappedHeading>

				<expandedContent>
					<m:Label text="Fill your insurance details"/>
				</expandedContent>

				<snappedTitleOnMobile>
					<m:Title text="Insurance Details"/>
				</snappedTitleOnMobile>

				<actions>
					<m:Button text="Save and Continue" enabled="true" icon="sap-icon://save" type="Emphasized" press="onPressSaveInsuranceDetails"></m:Button>
				</actions>
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>

		<headerContent>
			<m:FlexBox wrap="Wrap" fitContainer="true">
				<m:Avatar class="sapUiMediumMarginEnd sapUiSmallMarginBottom" src="sap-icon://insurance-house" displayShape="Circle" initials="ID" displaySize="L"/>

				<m:VBox class="sapUiLargeMarginEnd sapUiSmallMarginBottom">
					<m:Title level="H4" titleStyle="H4" text="Plant" class="sapUiTinyMarginBottom">
					</m:Title>
					<m:Label text="{plantsModel>/plant}"/>
				</m:VBox>

				<m:VBox class="sapUiSmallMarginBottom sapUiLargeMarginEnd">
					<m:Title level="H4" titleStyle="H4" text="Property" class="sapUiTinyMarginBottom" />
					<m:Label text="{plantsModel>/property}"/>
				</m:VBox>

				<m:VBox class="sapUiSmallMarginBottom">
					<m:Title level="H4" titleStyle="H4" text="Address" class="sapUiTinyMarginBottom" />
					<m:Label wrapping="true" text="{plantsModel>/address}"/>
				</m:VBox>
			</m:FlexBox>
		</headerContent>

		<sections>

			<ObjectPageSection titleUppercase="false" id="personalSection" title="Insurance Details">
				<subSections>
					<ObjectPageSubSection id="personalSectionSS1"  titleUppercase="false">
						<blocks>
              <f:SimpleForm id="basicDetails" editable="true" layout="ResponsiveGridLayout" labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2" singleContainerFullSize="false">
                <f:content>
                    <core:Title text="" visible="false"/>
                    <m:Label text="{i18n>finrepnum1}" />
                    <m:Input change="onDetectChange" id="finrep1"  maxLength="10"   value="{plantBasicDetailsModel>/FinrepNum1}" />
                    <m:Label text="{i18n>finrepnum2}" />
                    <m:Input change="onDetectChange" id="finrep2" maxLength="10" value="{plantBasicDetailsModel>/FinrepNum2}" />

                    <m:Label text="{i18n>insPrem2}" />
                    <m:Input change="onDetectChange" maxLength="10" id="insprem2" valueStateText="Value cannot be blank" valueState="{/InsPrem2000}" value="{plantBasicDetailsModel>/InsPrem2000}" />

                    <m:Label text="{i18n>insPrem3}" />
                    <m:Input change="onDetectChange" maxLength="10"  id="insprem3" valueStateText="Value cannot be blank" valueState="{/InsPrem3000}" value="{plantBasicDetailsModel>/InsPrem3000}" />

                    <m:Label text="{i18n>insPrem4}" />
                    <m:Input change="onDetectChange" maxLength="10"  id="insprem4" valueStateText="Value cannot be blank" valueState="{/InsPrem4000}" value="{plantBasicDetailsModel>/InsPrem4000}" />

                    <m:Label text="{i18n>insPrem5}" />
                    <m:Input change="onDetectChange" maxLength="10"  id="insprem5" valueStateText="Value cannot be blank" valueState="{/InsPrem5000}" value="{plantBasicDetailsModel>/InsPrem5000}" />

                    <m:Label text="{i18n>finrepnum7}" />
                    <m:Input change="onDetectChange" id="finrep7" maxLength="10"  value="{plantBasicDetailsModel>/FinrepNum7}" />

                    <m:Label text="{i18n>finrepnum8}" />
                    <m:Input change="onDetectChange" id="finrep8" maxLength="10"  value="{plantBasicDetailsModel>/FinrepNum8}" />

                    <m:Label text="{i18n>finrepnum9}" />
                    <m:Input change="onDetectChange" id="finrep9" maxLength="10"  value="{plantBasicDetailsModel>/FinrepNum9}" />
                    <m:Label required="true" text="{i18n>eRentalMax}" />
                    <m:Input change="onDetectChange" id="eRental" valueStateText="Value cannot be blank" valueState="{/ErentalMaxDays}" value="{plantBasicDetailsModel>/ErentalMaxDays}" />

                    <core:Title text="" />

                    <m:Label text="{i18n>cddStartDateN}" />
                    <m:DatePicker change="onDetectChange" id="startdate" placeholder="Choose Date.." valueStateText="Value cannot be blank" valueState="{/CddStartDate}" value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate2',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" displayFormat="yyyy-MM-dd" />

                    <m:Label text="{i18n>finrepD3}" />
                    <m:DatePicker change="onDetectChange" id="finrepD3" placeholder="Choose Date.." value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate3',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd"/>

                    <m:Label text="{i18n>finrepD4}" />
                    <m:DatePicker change="onDetectChange" id="finrepD4" placeholder="Choose Date.." value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate4',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" />
                    <m:Label text="{i18n>finrepD5}" />
                    <m:DatePicker change="onDetectChange" id="finrepD5" placeholder="Choose Date.." value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate5',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd"/>

                    <m:Label text="{i18n>finrepD6}" />
                    <m:DatePicker change="onDetectChange" id="finrepD6" placeholder="Choose Date.." value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate6',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" />

                    <m:Label text="{i18n>finrepD7}" />
                    <m:DatePicker change="onDetectChange" id="finrepD7" placeholder="Choose Date.." value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate7',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" />
                    <m:Label text="{i18n>finrepD8}" />
                    <m:DatePicker change="onDetectChange" id="finrepD8" value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate8',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" placeholder="Choose Date.."/>
                    <m:Label text="{i18n>finrepD9}" />
                    <m:DatePicker change="onDetectChange" id="finrepD9" value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate9',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" placeholder="Choose Date.." />
                    <m:Label text="{i18n>finrepD10}" />
                    <m:DatePicker change="onDetectChange" id="finrepD10" value="{ 
                        path: 'plantBasicDetailsModel>/FinrepDate10',
                        type: 'sap.ui.model.type.Date'  
                      }" valueFormat="yyyy-MM-dd" placeholder="Choose Date.."/>

                </f:content>
            </f:SimpleForm>
						</blocks>
					</ObjectPageSubSection>
				</subSections>
			</ObjectPageSection>

		</sections>
	</ObjectPageLayout>
</mvc:View>