                         <Label required="true" text="{i18n>gateSaturdayClosedHr}" />
                         <TimePicker id="gateSatClose" value="{parts: ['plantBasicDetailsModel>/GateSaturdayCloseHr'], formatter: '.formatter.formatGateTimeClose'}" valueFormat="HH:mm:ss" displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />

                         <Label required="true" text="{i18n>gateThanksGivingClosedHr}" />
                         <TimePicker id="gateThanksClose" value="{parts: ['plantBasicDetailsModel>/GateThanksgivingCloseHr'], formatter: '.formatter.formatGateTimeClose'}" valueFormat="HH:mm:ss" displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />

                         <Label required="true" text="{i18n>gateChristmasClosedHr}" />
                         <TimePicker id="gateChrisClose" value="{parts: ['plantBasicDetailsModel>/GateChristmasCloseHr'], formatter: '.formatter.formatGateTimeClose'}"  valueFormat="HH:mm:ss"  displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />

