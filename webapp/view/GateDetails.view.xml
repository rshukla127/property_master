                         <Label required="true" text="{i18n>gateThanksGivingOpenHr}" />
                         <TimePicker id="gateThanksOpen" value="{parts: ['plantBasicDetailsModel>/GateThanksgivingOpenHr'], formatter: '.formatter.formatGateTimeOpen'}" valueFormat="HH:mm:ss"  displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />

                         <Label required="true" text="{i18n>gateChristmasOpenHr}" />
                         <TimePicker id="gateChrisOpen" value="{parts: ['plantBasicDetailsModel>/GateChristmasOpenHr'], formatter: '.formatter.formatGateTimeOpen'}" displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />

                         <Label required="true" text="{i18n>gateNewYearOpenHr}" />
                         <TimePicker id="gateNewYearOpen" value="{parts: ['plantBasicDetailsModel>/GateNewyearOpenHr'], formatter: '.formatter.formatGateTimeOpen'}" valueFormat="HH:mm:ss"  displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />

                         <Label required="true" text="{i18n>gateAccessZone1}" />
                         <Input id="gateac1" value="{plantBasicDetailsModel>/GateAccessZone01}" />

                         <Label required="true" text="{i18n>gateAccessZone2}" />
                         <Input id="gateac2" value="{plantBasicDetailsModel>/GateAccessZone02}" />
                         <Label required="true" text="{i18n>gateAccessZone3}" />
                         <Input id="gateac3" value="{plantBasicDetailsModel>/GateAccessZone03}" />
                         <Label required="true" text="{i18n>gateAccessZone4}" />
                         <Input id="gateac4" value="{plantBasicDetailsModel>/GateAccessZone04}" />
                         <Label required="true" text="{i18n>gateAccessZone5}" />
                         <Input id="gateac5" value="{plantBasicDetailsModel>/GateAccessZone05}" />

                         <core:Title text="" />

                         <Label required="true" text="{i18n>gateSundayClosedHr}" />
                         <TimePicker id="gateSunClose" value="{parts: ['plantBasicDetailsModel>/GateSundayCloseHr'], formatter: '.formatter.formatGateTimeClose'}" valueFormat="HH:mm:ss"  displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />
                         <Label required="true" text="{i18n>gateMondayClosedHr}" />
