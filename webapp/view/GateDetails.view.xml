                         <Label required="true" text="{i18n>gateNewYearClosedHr}" />
                         <TimePicker id="gateNewYearClose" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateNewyearCloseHr'], formatter: '.formatter.formatGateTimeClose'}" valueFormat="HH:mm:ss" displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />

                         <Label  text="{i18n>gateAccessZone5}" />
                         <Input id="gateac5" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateAccessZone05'], formatter: '.formatter.formatGetAccess5'}" />

                         <Label  text="{i18n>gateAccessZone6}" />
                         <Input id="gateac6" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateAccessZone06'], formatter: '.formatter.formatGetAccess6'}" />

                         <Label  text="{i18n>gateAccessZone7}" />
                         <Input id="gateac7" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateAccessZone07'], formatter: '.formatter.formatGetAccess7'}" />

                         <Label text="{i18n>gateAccessZone8}" />
                         <Input id="gateac8" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateAccessZone07'], formatter: '.formatter.formatGetAccess8'}" />

                         <Label  text="{i18n>gateAccessZone9}" />
                         <Input id="gateac9" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateAccessZone09'], formatter: '.formatter.formatGetAccess9'}" />

                         <Label  text="{i18n>gateAccessZone10}" />
                         <Input id="gateac10" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateAccessZone10'], formatter: '.formatter.formatGetAcces10'}" />

                    </f:content>
               </f:SimpleForm>

               <VBox alignContent="End" alignItems="End">
                    <Button text="Save and Continue" enabled="true" type="Emphasized" press="onPressSaveGateDetails"></Button>
               </VBox>
          </VBox>
     </ScrollContainer>
</mvc:View>