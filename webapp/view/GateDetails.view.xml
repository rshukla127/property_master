                         <Label required="true" text="{i18n>gateAccessZone8}" />
                         <Input id="gateac8" value="{plantBasicDetailsModel>/GateAccessZone08}" />

                         <Label required="true" text="{i18n>gateAccessZone9}" />
                         <Input id="gateac9" value="{plantBasicDetailsModel>/GateAccessZone09}" />

                         <Label required="true" text="{i18n>gateAccessZone10}" />
                         <Input id="gateac10" value="{plantBasicDetailsModel>/GateAccessZone10}" />

                    </f:content>
               </f:SimpleForm>

               <VBox alignContent="End" alignItems="End">
                    <Button text="Save and Continue" enabled="true" type="Emphasized" press="onPressSaveGateDetails"></Button>
               </VBox>
          </VBox>
     </ScrollContainer>
</mvc:View>