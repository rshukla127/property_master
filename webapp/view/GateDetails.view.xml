                    <f:content>
                         <core:Title text="" visible="false"/>
                         <Label required="true" text="{i18n>gateSundayOpenHr}" />
                         <TimePicker change="onDetectChange" id="gateSunOpen" value="{parts: ['plantBasicDetailsModel>/GateSundayOpenHr'], formatter: '.formatter.formatGateTimeOpen'}" valueFormat="HH:mm:ss" displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />
                         <Label required="true" text="{i18n>gateMondayOpenHr}" />
