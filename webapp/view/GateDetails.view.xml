                         <Label required="true" text="{i18n>gateThursdayClosedHr}" />
                         <TimePicker id="gateThuClose" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateThursdayCloseHr'], formatter: '.formatter.formatGateTimeClose'}" valueFormat="HH:mm:ss" displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />
                         <Label required="true" text="{i18n>gateFriadayClosedHr}" />
                         <TimePicker id="gateFriClose" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateFridayCloseHr'], formatter: '.formatter.formatGateTimeClose'}" valueFormat="HH:mm:ss" displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />
                         <Label required="true" text="{i18n>gateSaturdayClosedHr}" />
                         <TimePicker id="gateSatClose" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateSaturdayCloseHr'], formatter: '.formatter.formatGateTimeClose'}" valueFormat="HH:mm:ss" displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />

                         <Label required="true" text="{i18n>gateThanksGivingClosedHr}" />
                         <TimePicker id="gateThanksClose" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateThanksgivingCloseHr'], formatter: '.formatter.formatGateTimeClose'}" valueFormat="HH:mm:ss" displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />

                         <Label required="true" text="{i18n>gateChristmasClosedHr}" />
