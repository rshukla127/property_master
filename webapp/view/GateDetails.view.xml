                         <Label required="true" text="{i18n>gateThursdayOpenHr}" />
                         <TimePicker id="gateThuOpen" value="{parts: ['plantBasicDetailsModel>/GateThursdayOpenHr'], formatter: '.formatter.formatGateTimeOpen'}" valueFormat="HH:mm:ss"  displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />

                         <Label required="true" text="{i18n>gateFriadayOpenHr}" />
                         <TimePicker id="gateFriOpen" value="{parts: ['plantBasicDetailsModel>/GateFridayOpenHr'], formatter: '.formatter.formatGateTimeOpen'}" valueFormat="HH:mm:ss"  displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />
