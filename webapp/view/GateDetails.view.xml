                         <Label  text="{i18n>gateAccessZone0}" />
                         <Input id="gateac0" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateAccessZone00'], formatter: '.formatter.formatGetAccess0'}" />

                         <Label  text="{i18n>gateAccessZone1}" />
                         <Input id="gateac1" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateAccessZone01'], formatter: '.formatter.formatGetAccess1'}" />
                         <Label  text="{i18n>gateAccessZone2}" />
                         <Input id="gateac2" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateAccessZone02'], formatter: '.formatter.formatGetAccess2'}" />
                         <Label  text="{i18n>gateAccessZone3}" />
                         <Input id="gateac3" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateAccessZone03'], formatter: '.formatter.formatGetAccess3'}" />
                         <Label  text="{i18n>gateAccessZone4}" />
                         <Input id="gateac4" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateAccessZone04'], formatter: '.formatter.formatGetAccess4'}" />

                         <core:Title text="" />

                         <Label required="true" text="{i18n>gateSundayClosedHr}" />
                         <TimePicker id="gateSunClose" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateSundayCloseHr'], formatter: '.formatter.formatGateTimeClose'}" valueFormat="HH:mm:ss" displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />
                         <Label required="true" text="{i18n>gateMondayClosedHr}" />
                         <TimePicker id="gateMonClose" change="onDetectChange" value="{parts: ['plantBasicDetailsModel>/GateMondayCloseHr'], formatter: '.formatter.formatGateTimeClose'}" valueFormat="HH:mm:ss" displayFormat="HH:mm:ss" support2400="true" maskMode="On" placeholder="Choose Time.." />

                         <Label required="true" text="{i18n>gateTuesdayClosedHr}" />
