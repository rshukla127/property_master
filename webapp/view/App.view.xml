<mvc:View xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    xmlns:f="sap.f" xmlns:tnt="sap.tnt"
   controllerName="com.public.storage.pao.controller.App" height="100%">
    <tnt:ToolPage id="app" busy="{appView>/busy}" busyIndicatorDelay="{appView>/delay}" class="sapUiDemoToolPage">
     <tnt:header>
           <tnt:ToolHeader>
                <!--				<Input id="tableState" visible="false"/>-->
                <Button id="sideNavigationToggleButton" icon="sap-icon://menu2" type="Transparent" press=".onToggleSideNavPress" tooltip="{i18n>navigationToggleButtonTooltip}">
                    <layoutData>
                        <OverflowToolbarLayoutData priority="NeverOverflow" />
                    </layoutData>
                </Button>
                <Image
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="{imageModel>/path}/Images/public.png"
                />
           
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:sideContent>
            <tnt:SideNavigation expanded="true" itemSelect=".onItemSelect">
                <tnt:NavigationList id="navList" items="{
						path: 'sideNavigation>/navigation',
						templateShareable: false,
						factory: '.navItemFactory'
					}" />
                <!--				<tnt:fixedItem>-->
                <!--					<tnt:NavigationList-->
                <!--						items="{-->
                <!--							path: 'sideNavigation>/fixedNavigation',-->
                <!--							templateShareable: false,-->
                <!--							factory: '.navItemFactory'-->
                <!--						}">-->
                <!--					</tnt:NavigationList>-->
                <!--				</tnt:fixedItem>-->
            </tnt:SideNavigation>
        </tnt:sideContent>
        <tnt:mainContents>
            <App id="mainContents" />
            <Input id="tableState" visible="false" />
        </tnt:mainContents>
      </tnt:ToolPage>
</mvc:View>
