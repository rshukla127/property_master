<mvc:View controllerName="com.public.storage.pao.controller.BUDetails"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns:l="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <ScrollContainer height="100%" width="100%" vertical="true" focusable="true">



        <VBox class="sapUiSmallMargin">
            <f:SimpleForm id="propStructure" editable="true" layout="ResponsiveGridLayout" title="{i18n>propBUDetail}" labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2" singleContainerFullSize="false">
                <f:content>
                    <core:Title text="" visible="false"/>
                    <Label required="true" text="{i18n>fieldSupervisorName}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/DirectPhoneNo}" />
                    <Label required="true" text="{i18n>filedSupOfficeName}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/FaxNumber}" />

                    <Label required="true" text="{i18n>fieldSupMobile}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/TollFreeNumber}" />

                    <Label required="true" text="{i18n>fieldSupFax}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/Network1IpAddress}" />

                    <Label required="true" text="{i18n>fieldSupEmail}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/TollFreeNumber}" />

                    <Label text="{i18n>evpZone}" />
                    <Input value="{basicDetailsModel>/0/Network1IpAddress}" />

                    <Label text="{i18n>division}" />
                    <Input value="{basicDetailsModel>/0/Network1IpAddress}" />

                    <Label text="{i18n>seniorRegion}" />
                    <Input value="{basicDetailsModel>/0/Network1IpAddress}" />

                    <Label text="{i18n>region}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/Network1IpAddress}" />

                    <Label text="{i18n>seniorDistrict}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/Network1IpAddress}" />
                    <core:Title text="" />
                    <Label text="{i18n>district}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/Network1IpAddress}" />
                    <Label text="{i18n>deliTennent}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/Network1IpAddress}" />
                    <Label text="{i18n>pmwage}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/Network1IpAddress}" />
                    <Label text="{i18n>fieldOffice}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/Network1IpAddress}" />
                    <Label text="{i18n>vpFacilities}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/Network1IpAddress}" />
                    <Label text="{i18n>seniorReegion}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/Network1IpAddress}" />
                    <Label text="{i18n>regionFacility}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/Network1IpAddress}" />
                    <Label text="{i18n>seniorFacility}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/Network1IpAddress}" />
                    <Label text="{i18n>facility}" />
                    <Input showValueHelp="true" value="{basicDetailsModel>/0/Network1IpAddress}" />

                </f:content>
            </f:SimpleForm>
            <VBox alignContent="End" alignItems="End">
                <Button text="Save and Continue" enabled="false" type="Emphasized" press="onPressSaveBasicDetails"></Button>
            </VBox>
        </VBox>


    </ScrollContainer>
</mvc:View>