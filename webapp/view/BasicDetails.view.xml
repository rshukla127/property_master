<mvc:View controllerName="com.public.storage.pao.controller.BasicDetails"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
	xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form"
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ScrollContainer height="100%" width="100%" vertical="true" focusable="true">



		<VBox class="sapUiSmallMargin">
			<f:SimpleForm id="basicDetails" editable="true" layout="ResponsiveGridLayout" title="{i18n>propBasDetails} (Plant - {plantsModel>/plant} &amp; Property - {plantBasicDetailsModel>/LegacyPropertyNumber} - {plantsModel>/address})" labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2" singleContainerFullSize="false">
				<f:content>
					<core:Title text="" visible="false"/>
					<Label text="{i18n>direPhoneNo}" />
					<Input maxLength="30" id="dirPhoneNo" value="{plantBasicDetailsModel>/DirectPhoneNo}" enabled="false" />
					<Label text="{i18n>faxNumber}" />
					<Input maxLength="30" id="faxNo" enabled="false" value="{basicDetailPlantModel>/Faxnumber}" />

					<Label required="true" text="{i18n>tollfreeNo}" />
					<Input maxLength="30" valueStateText="Value cannot be blank" valueState="{/tollFreeNumber}" id="tollFree" value="{plantBasicDetailsModel>/TollFreeNumber}"/>

					<Label required="true" text="{i18n>netIpAddress}" />
					<Input maxLength="30" id="network1" valueStateText="Value cannot be blank" valueState="{/network1IpAddress}" value="{plantBasicDetailsModel>/Network1IpAddress}"/>

					<Label required="true" text="{i18n>kioskProp}" />
					<Select id="kiskProp" valueStateText="Value cannot be blank" valueState="{/KioskProperty}" selectedKey="{parts: ['plantBasicDetailsModel>/KioskProperty'], formatter: '.formatter.getSelectedKeyNo'}">
						<items>																								
							<core:Item text="Yes" key="Y" />
							<core:Item text="No" key="N"/>

						</items>
					</Select>

					<Label text="{i18n>budPropNumber}" />
					<Input maxLength="5" id="budProp" value="{plantBasicDetailsModel>/BuddyPropertyNumber}" valueHelpOnly="true"/>
					<Label text="{i18n>retailSqFootage}" />
					<Input maxLength="5" id="retailStoreSq" value="{plantBasicDetailsModel>/RetailStoreSquareFootage}" />

					<Label text="{i18n>historicalProp}" />
					<Input maxLength="10" id="histProp" value="{plantBasicDetailsModel>/HistoricalProperty}" type="Url" />
					<Label text="{i18n>psaProp}" />
					<Input maxLength="10" id="psaProp" value="{plantBasicDetailsModel>/PsaProperty}" />
					<Label text="{i18n>transForm}" />
					<Input maxLength="10" id="transF" value="{plantBasicDetailsModel>/TransferFrom}" />

					<core:Title text="" />

					<Label required="true" text="{i18n>publishedPhoneNo}" />
					<Input maxLength="30" valueStateText="Value cannot be blank" valueState="{/publishedPhoneNo}" id="pubPhone" value="{plantBasicDetailsModel>/PublishedPhoneNo}" />
					<Label text="{i18n>propEmailAdd}" />
					<Input id="email" enabled="false" value="{basicDetailPlantModel>/Email}" />

					<Label text="{i18n>localPhoneNo}" />
					<Input maxLength="30" valueStateText="Value cannot be blank" valueState="{/localPhoneNumber}" required="true" id="localPh" value="{plantBasicDetailsModel>/LocalPhoneNumber}" type="Url" />
					<Label required="true" text="{i18n>network2Ip}" />
					<Input id="network2" valueStateText="Value cannot be blank" valueState="{/network2IpAddress}" value="{plantBasicDetailsModel>/Network2IpAddress}" />

					<Label text="{i18n>kisokActivDate}" labelFor="kisok" />
					<DatePicker valueStateText="Value cannot be blank" valueState="{/KioskActiveDate}" required="true" id="kisok" value="{ 
					path: 'plantBasicDetailsModel>/KioskActiveDate',
					type: 'sap.ui.model.type.Date'  
				  }" valueFormat="yyyy-MM-dd" placeholder="Choose Date.."/>
					<Label text="{i18n>offSqFoootage}" />
					<Input id="offSq" value="{plantBasicDetailsModel>/RetailStorageSize}" />
					<Label text="{i18n>retailSqSize}" />
					<Input id="retailST" value="{plantBasicDetailsModel>/RetailStorageSize}" />
					<Label text="{i18n>histOwner}" />
					<Input maxLength="10" id="histOwner" value="{plantBasicDetailsModel>/HistoricalOwner}" />
					<Label text="{i18n>psaOwner}" />
					<Input maxLength="10" id="PsaOwner" value="{plantBasicDetailsModel>/PsaOwner}" />
					<Label required="true" text="{i18n>geoCode}" />
					<Input maxLength="10" valueStateText="Value cannot be blank" valueState="{/GeoCode}" id="geo" value="{plantBasicDetailsModel>/GeoCode}" />

				</f:content>
			</f:SimpleForm>
			<VBox alignContent="End" alignItems="End">
				<Button text="Save and Continue" enabled="true" type="Emphasized" press="onPressSaveBasicDetails"></Button>
			</VBox>
		</VBox>
	</ScrollContainer>
</mvc:View>