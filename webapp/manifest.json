{
  "_version": "1.49.0",
  "sap.app": {
    "id": "com.public.storage.pao",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.10.2",
      "toolsId": "12e9f91e-d4ce-4f6b-bf5f-1651dafbd3fa"
    },
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZMM_PROPERTY_MASTER_SRV/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "localUri": "localService/metadata.xml",
          "odataVersion": "2.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": true,
    "dependencies": {
      "minUI5Version": "1.116.0",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "com.public.storage.pao.i18n.i18n"
        }
      },
      "mainModel": {
        "type":"sap.ui.model.odata.v2.ODataModel",
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "defaultBindingMode": "OneWay"
        }
      },
      "sideNavigation": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/sideNavigationContent.json",
				"preload": true
			},
      "plantModel":{
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/mockdata.json",
				"preload": true
			}
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "com.public.storage.pao.view",
        "controlAggregation": "pages",
        "controlId": "mainContents",
        "bypassed": {
          "target": [
            "notFound"
          ]
        }
      },
      "routes": [
        {
          "name": "home",
          "pattern": "",
          "target": ["home"]
        },
        {
          "name": "basicDetails",
          "pattern": "basicDetails",
          "target": ["basicDetails"]
        },
        {
          "name": "buDetails",
          "pattern": "buDetails",
          "target": ["buDetails"]
        },
        {
          "name": "propOtherDetails",
          "pattern": "propOtherDetails",
          "target": ["propOtherDetails"]
        },
        {
          "name": "marketingDetails",
          "pattern": "marketingDetails",
          "target": ["marketingDetails"]
        },
        {
          "name": "propStructure",
          "pattern": "propStructure",
          "target": ["propStructure"]
        },
        {
          "name": "propSuppDeatils",
          "pattern": "propSuppDeatils",
          "target": ["propSuppDeatils"]
        },
        {
          "name": "workingHours",
          "pattern": "workingHours",
          "target": ["workingHours"]
        }
      ],
      "targets": {
        "home": {
          "viewType": "XML",
          "viewId": "Main",
          "viewName": "Main"
        },
        "basicDetails": {
          "viewType": "XML",
          "viewId": "basicDetails",
          "viewName": "BasicDetails"
        },
        "buDetails": {
          "viewType": "XML",
          "viewId": "buDetails",
          "viewName": "BUDetails"
        },
        "propOtherDetails": {
          "viewType": "XML",
          "viewId": "propOtherDetails",
          "viewName": "OtherDetails"
        },
        "marketingDetails": {
          "viewType": "XML",
          "viewId": "marketingDetails",
          "viewName": "MarketingDetails"
        },
        "propStructure": {
          "viewType": "XML",
          "viewId": "propStructure",
          "viewName": "PropertyStructure"
        },
        "propSuppDeatils": {
          "viewType": "XML",
          "viewId": "propSuppDeatils",
          "viewName": "SupplementalDetails"
        },

        "workingHours": {
          "viewType": "XML",
          "viewId": "workingHours",
          "viewName": "WorkingHours"
        }
      }
    },
    "rootView": {
      "viewName": "com.public.storage.pao.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}
