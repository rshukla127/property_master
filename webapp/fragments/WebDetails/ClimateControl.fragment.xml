<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.MarketingDetails" >
	<SelectDialog class="sapUiSizeCompact"
		id="climateC"
		title="Climate Control"
		items="{climateControlModel>/}"
		search="onValueHelpDialogSearchPlant"
		confirm="onValueHelpDialogClose"
		cancel="onValueHelpDialogClosePlant">
		<StandardListItem
			class="sapUiSizeCompact"
			iconDensityAware="false"
			iconInset="false"
			title="{climateControlModel>Code}"
			description="{climateControlModel>Description}"/>
	</SelectDialog>
</core:FragmentDefinition>