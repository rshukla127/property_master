<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.MarketingDetails" >
	<SelectDialog class="sapUiSizeCompact"
		id="mstatsArea"
		title="Metro Statistical Area"
		items="{metroStatModel>/}"
		search="onValueHelpDialogSearchMetroStats"
		confirm="onValueHelpDialogClose"
		cancel="onValueHelpDialogClose">
		<StandardListItem
			class="sapUiSizeCompact"
			iconDensityAware="false"
			iconInset="false"
			title="{metroStatModel>Code}"
			description="{metroStatModel>Description}"/>
	</SelectDialog>
</core:FragmentDefinition>