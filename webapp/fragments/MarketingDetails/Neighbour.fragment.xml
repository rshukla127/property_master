<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.MarketingDetails" >
	<SelectDialog class="sapUiSizeCompact"
		id="neighbour"
		title="Neighbourhood"
		items="{NeighborhoodModel>/}"
		search="onValueHelpDialogSearchNeighbour"
		confirm="onValueHelpDialogClose"
		cancel="onValueHelpDialogClose">
		<StandardListItem
			class="sapUiSizeCompact"
			iconDensityAware="false"
			iconInset="false"
			title="{NeighborhoodModel>Code}"
			description="{NeighborhoodModel>Description}"/>
	</SelectDialog>
</core:FragmentDefinition>