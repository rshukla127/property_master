<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.PropertyStructure" >
	<SelectDialog class="sapUiSizeCompact"
		id="faciMgr"
		title="Facility Mgr"
		items="{FacilitiesMgrModel>/}"
		search="onValueHelpDialogSearchPlant"
		confirm="onValueHelpDialogClosePlant"
		cancel="onValueHelpDialogClosePlant">
		<StandardListItem
			class="sapUiSizeCompact"
			iconDensityAware="false"
			iconInset="false"
			title="{FacilitiesMgrModel>Code}"
			description="{FacilitiesMgrModel>Description}"/>
	</SelectDialog>
</core:FragmentDefinition>