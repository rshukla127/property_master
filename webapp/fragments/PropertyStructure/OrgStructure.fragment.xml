<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.PropertyStructure" >
	<SelectDialog class="sapUiSizeCompact"
		id="orgstructure"
		title="Org Structure"
		items="{orgStructureModel>/}"
		search="onValueHelpDialogSearchOrgStruct"
		confirm="onValueHelpDialogClose"
		cancel="onValueHelpDialogClosePlant">
		<StandardListItem
			class="sapUiSizeCompact"
			iconDensityAware="false"
			iconInset="false"
			title="{orgStructureModel>Code}"
			description="{orgStructureModel>Description}"/>
	</SelectDialog>
</core:FragmentDefinition>