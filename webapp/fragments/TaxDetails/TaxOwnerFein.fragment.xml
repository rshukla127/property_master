<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.TaxDetails" >
	<SelectDialog class="sapUiSizeCompact"
		id="txOwnerFein"
		title="Tax Owner FEIN"
		items="{taxOwnerFeinModel>/}"
		search="onValueHelpDialogSearchPlant"
		confirm="onValueHelpDialogClose"
		cancel="onValueHelpDialogClose">
		<StandardListItem
			class="sapUiSizeCompact"
			iconDensityAware="false"
			iconInset="false"
			title="{taxOwnerFeinModel>Code}"
			description="{taxOwnerFeinModel>Description}"/>
	</SelectDialog>
</core:FragmentDefinition>