<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.TaxDetails" >
	<SelectDialog class="sapUiSizeCompact"
		id="txFillingEntity"
		title="Tax Filling Entity"
		items="{taxFilingModel>/}"
		search="onValueHelpDialogSearchSolarENtity"
		confirm="onValueHelpDialogClose"
		cancel="onValueHelpDialogClose">
		<StandardListItem
			class="sapUiSizeCompact"
			iconDensityAware="false"
			iconInset="false"
			title="{taxFilingModel>Code}"
			description="{taxFilingModel>Description}"/>
	</SelectDialog>
</core:FragmentDefinition>