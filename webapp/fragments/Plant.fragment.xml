<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.Main" >
	<SelectDialog class="sapUiSizeCompact"
		id="plantFragment"
		title="Plants"
		items="{plantsModel>/}"
		search="onValueHelpDialogSearchPlant"
		confirm="onValueHelpDialogClosePlant"
		cancel="onValueHelpDialogClosePlant">
		<StandardListItem
			class="sapUiSizeCompact"
			iconDensityAware="false"
			iconInset="false"
			title="{plantsModel>Werks}"/>
	</SelectDialog>
</core:FragmentDefinition>