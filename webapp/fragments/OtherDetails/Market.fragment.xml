<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.OtherDetails" >
	<SelectDialog class="sapUiSizeCompact"
		id="market"
		title="Market Class"
		items="{marketClassSetModel>/}"
		search="onValueHelpDialogSearchPlant"
		confirm="onValueHelpDialogClose"
		cancel="onValueHelpDialogClose">
		<StandardListItem
			class="sapUiSizeCompact"
			iconDensityAware="false"
			iconInset="false"
			title="{marketClassSetModel>Code}"
			description="{marketClassSetModel>Description}"/>
	</SelectDialog>
</core:FragmentDefinition>