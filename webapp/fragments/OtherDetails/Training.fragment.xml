<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.Training" >
	<SelectDialog class="sapUiSizeCompact"
		id="training"
		title="Training Proffesionals"
		items="{keyTrainingModel>/}"
		search="onValueHelpDialogSearchPlant"
		confirm="onValueHelpDialogClose"
		cancel="onValueHelpDialogClose">
		<StandardListItem
			class="sapUiSizeCompact"
			iconDensityAware="false"
			iconInset="false"
			title="{keyTrainingModel>Code}"
			description="{keyTrainingModel>Description}"/>
	</SelectDialog>
</core:FragmentDefinition>