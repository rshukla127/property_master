<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.Main">
	<!-- <SelectDialog id="tennentPC" title="Tennent PC" items="{tennentModel>/}" search="onValueHelpDialogSearchPC" confirm="onValueHelpDialogClosePC" cancel="onValueHelpDialogCloseProperty"> <StandardListItem iconDensityAware="false" iconInset="false" title="{tennentModel>Prctr}"/>
	</SelectDialog> -->

	<TableSelectDialog id="tennentPC" noDataText="No PC Found" title="Tennent PC" search="onValueHelpDialogSearchPC" confirm="onValueHelpDialogClosePC" cancel="onValueHelpDialogCloseTenantPC" items="{
		path : 'tennentModel>/',
		sorter : {
			path : 'Name1',
			descending : false
		}
	}">
		<ColumnListItem vAlign="Middle">
			<cells>
				<Text text="{tennentModel>Prctr}"/>
				<Text text="{tennentModel>Name2}" />
				<Text text="{tennentModel>Bukrs}" />
				<Text  text="{parts: ['tennentModel>Datab'], formatter: '.formatter.formatDate'}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="Profit Center" />
				</header>
			</Column>
			<Column minScreenWidth="Desktop" width="12em" demandPopin="true">
				<header>
					<Text text="Long Description" />
				</header>
			</Column>
			<Column minScreenWidth="Desktop" width="12em" demandPopin="true">
				<header>
					<Text text="Company Code" />
				</header>
			</Column>
			<Column minScreenWidth="Desktop" demandPopin="true" width="12em">
				<header>
					<Text text="Valid From" />
				</header>
			</Column>


		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>