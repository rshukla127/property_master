<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.Main">
	<!-- <SelectDialog id="storagePC" title="Storage PC" items="{storageModel>/}" search="onValueHelpDialogSearchPC" confirm="onValueHelpDialogClosePC" cancel="onValueHelpDialogCloseProperty"> <StandardListItem iconDensityAware="false" iconInset="false" title="{storageModel>Prctr}"/>
	</SelectDialog> -->

	<TableSelectDialog id="storagePC" noDataText="No PC Found" title="Storage PC" search="onValueHelpDialogSearchPC" confirm="onValueHelpDialogClosePC" cancel="onValueHelpDialogCloseStoragePC" items="{
		path : 'storageModel>/',
		sorter : {
			path : 'Name1',
			descending : false
		}
	}">
		<ColumnListItem vAlign="Middle">
			<cells>
				<Text text="{storageModel>Prctr}"/>
				<Text text="{storageModel>Name2}" />
				<Text text="{storageModel>Bukrs}" />

				<Text text="{parts: ['storageModel>Datab'], formatter: '.formatter.formatDate'}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="Profit Center" />
				</header>
			</Column>

			<Column minScreenWidth="Desktop" width="12em" demandPopin="true">
				<header>
					<Text text="Long Description" />
				</header>
			</Column>
			<Column minScreenWidth="Desktop" width="12em" demandPopin="true">
				<header>
					<Text text="Company Code" />
				</header>
			</Column>
			<Column minScreenWidth="Desktop" demandPopin="true" width="12em">
				<header>
					<Text text="Valid From" />
				</header>
			</Column>


		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>