	<TableSelectDialog id="commPC" noDataText="No PC Found" title="Commercial PC" search="onValueHelpDialogSearchPC" confirm="onValueHelpDialogClosePC" cancel="onValueHelpDialogCloseProperty" items="{
		path : 'commPcModel>/',
		sorter : {
			path : 'Name1',
			descending : false
		}
	}">
	<ColumnListItem vAlign="Middle">
		<cells>
			<Text text="{commPcModel>Prctr}"/>
			<Text text="{commPcModel>Bukrs}" />
			<Text text="{commPcModel>Name1}" />
			<Text text="{commPcModel>Datab}" />
		</cells>
	</ColumnListItem>
	<columns>
		<Column minScreenWidth="Desktop" width="12em" demandPopin="true">
			<header>
				<Text text="Profit Center" />
			</header>
		</Column>
		<Column minScreenWidth="Desktop" width="12em" demandPopin="true">
			<header>
				<Text text="Company Code" />
			</header>
		</Column>
		<Column minScreenWidth="Desktop" width="12em" demandPopin="true" hAlign="End">
			<header>
				<Text text="Long Description" />
			</header>
		</Column>
		<Column minScreenWidth="Desktop" demandPopin="true" width="12em" hAlign="Center">
			<header>
				<Text text="Valid From" />
			</header>
		</Column>
		
		
	</columns>
</TableSelectDialog>
</core:FragmentDefinition>