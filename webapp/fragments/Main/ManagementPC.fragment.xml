	<TableSelectDialog id="mgmtPC" noDataText="No PC Found" title="Management PC" search="onValueHelpDialogSearchPC" confirm="onValueHelpDialogClosePC" cancel="onValueHelpDialogCloseProperty" items="{
		path : 'mgmtModel>/',
		sorter : {
			path : 'Name1',
			descending : false
		}
	}">
	<ColumnListItem vAlign="Middle">
		<cells>
			<Text text="{mgmtModel>Prctr}"/>
			<Text text="{mgmtModel>Bukrs}" />
			<Text text="{mgmtModel>Name1}" />
			<Text text="{mgmtModel>Datab}" />
		</cells>
	</ColumnListItem>
	<columns>
		<Column width="12em">
			<header>
				<Text text="Profit Center" />
			</header>
		</Column>
		<Column minScreenWidth="Desktop" width="12em" demandPopin="true">
			<header>
				<Text text="Company Code" />
			</header>
		</Column>
		<Column minScreenWidth="Desktop" width="12em" demandPopin="true" hAlign="End">
			<header>
				<Text text="Long Description" />
			</header>
		</Column>
		<Column minScreenWidth="Desktop" demandPopin="true" width="12em" hAlign="Center">
			<header>
				<Text text="Valid From" />
			</header>
		</Column>
		
		
	</columns>
</TableSelectDialog>
</core:FragmentDefinition>