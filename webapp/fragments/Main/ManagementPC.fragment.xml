<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.Main">
	<!-- <SelectDialog id="mgmtPC" title="Management PC" items="{mgmtModel>/}" search="onValueHelpDialogSearchPC" confirm="onValueHelpDialogClosePC" cancel="onValueHelpDialogCloseProperty"> <StandardListItem iconDensityAware="false" iconInset="false" title="{mgmtModel>Prctr}"/>
	</SelectDialog> -->

	<TableSelectDialog id="mgmtPC" noDataText="No PC Found" title="Management PC" search="onValueHelpDialogSearchPC" confirm="onValueHelpDialogClosePC" cancel="onValueHelpDialogManagementPC" items="{
		path : 'mgmtModel>/',
		sorter : {
			path : 'Name1',
			descending : false
		}
	}">
		<ColumnListItem vAlign="Middle">
			<cells>
				<Text text="{mgmtModel>Prctr}"/>

				<Text text="{mgmtModel>Name2}" />
				<Text text="{mgmtModel>Bukrs}" />
				<Text text="{parts: ['mgmtModel>Datab'], formatter: '.formatter.formatDate'}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="Profit Center" />
				</header>
			</Column>

			<Column minScreenWidth="Desktop" width="12em" demandPopin="true">
				<header>
					<Text text="Long Description" />
				</header>
			</Column>
			<Column minScreenWidth="Desktop" width="12em" demandPopin="true">
				<header>
					<Text text="Company Code" />
				</header>
			</Column>
			<Column minScreenWidth="Desktop" demandPopin="true" width="12em">
				<header>
					<Text text="Valid From" />
				</header>
			</Column>


		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>