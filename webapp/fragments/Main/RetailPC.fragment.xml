	<TableSelectDialog id="retailPC" noDataText="No PC Found" title="Retail PC"  search="onValueHelpDialogSearchPC" confirm="onValueHelpDialogClosePC" cancel="onValueHelpDialogCloseProperty" items="{
		path : 'retailpcModel>/',
		sorter : {
			path : 'Name1',
			descending : false
		}
	}">
	<ColumnListItem vAlign="Middle">
		<cells>
			<Text text="{retailpcModel>Prctr}"/>
				<Text text="{retailpcModel>Name2}" />
			<Text text="{retailpcModel>Bukrs}" />
			
			<Text text="{parts: ['retailpcModel>Datab'], formatter: '.formatter.formatDate'}" />
				
		</cells>
	</ColumnListItem>
	<columns>
		<Column width="12em">
			<header>
				<Text text="Profit Center" />
			</header>
		</Column>
		
		<Column minScreenWidth="Desktop" width="12em" demandPopin="true" >
			<header>
				<Text text="Long Description" />
			</header>
		</Column>
		<Column minScreenWidth="Desktop" width="12em" demandPopin="true">
			<header>
				<Text text="Company Code" />
			</header>
		</Column>
		<Column minScreenWidth="Desktop" demandPopin="true" width="12em" >
			<header>
				<Text text="Valid From" />
			</header>
		</Column>
		
		
	</columns>
</TableSelectDialog>
</core:FragmentDefinition>