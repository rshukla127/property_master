<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.Main" >
	<SelectDialog
		id="plantFragment"
		title="Products"
		items="{plantModel>/ProductCollection}"
		search="onValueHelpDialogSearchPlant"
		confirm="onValueHelpDialogClose"
		cancel=".onValueHelpDialogClose">
		<StandardListItem
			icon="{plantModel>ProductPicUrl}"
			iconDensityAware="false"
			iconInset="false"
			title="{plantModel>Name}"
			description="{plantModel>ProductId}" />
	</SelectDialog>
</core:FragmentDefinition>