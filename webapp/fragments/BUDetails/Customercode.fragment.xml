<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.MarketingDetails" >
	<SelectDialog class="sapUiSizeCompact"
		id="custcode"
		title="Customer Code"
		items="{custCodeModel>/}"
		search="onValueHelpDialogSearchCCcode"
		confirm="onValueHelpDialogClose"
		cancel="onValueHelpDialogClose">
		<StandardListItem
			class="sapUiSizeCompact"
			iconDensityAware="false"
			iconInset="false"
			title="{custCodeModel>Code}"
			description="{custCodeModel>Description}"/>
	</SelectDialog>
</core:FragmentDefinition>