<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" controllerName="com.public.storage.pao.controller.Main" >
	
	<TableSelectDialog id="propertyFragmentBuddy" noDataText="No Property Found" title="Select Buddy Property" search="onValueHelpDialogSearchProperty" confirm="onValueHelpDialogConfirmProperty" cancel="onValueHelpDialogCloseProperty" items="{
			path : 'plantsBuddyModel>/',
			sorter : {
				path : 'Werks',
				descending : false
			}
		}">
		<ColumnListItem vAlign="Middle">
			<cells>
				<Text text="{plantsBuddyModel>Werks}"/>
				<Text text="{plantsBuddyModel>Name1}" />
				<Text text="{plantsBuddyModel>Name2}" />
				<Text text="{plantsBuddyModel>Stras}" />
				<Text text="{plantsBuddyModel>Pstlz}" />
				
				<Text text="{plantsBuddyModel>Ort01}" />
					<Text text="{plantsBuddyModel>Land1}" />
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="Plant" />
				</header>
			</Column>
			<Column minScreenWidth="Tablet" demandPopin="true">
				<header>
					<Text text="Name" />
				</header>
			</Column>
			<Column minScreenWidth="Desktop" demandPopin="true" hAlign="End">
				<header>
					<Text text="Property" />
				</header>
			</Column>
			<Column minScreenWidth="Desktop" demandPopin="true" hAlign="Center">
				<header>
					<Text text="Street" />
				</header>
			</Column>
			<Column hAlign="End">
				<header>
					<Text text="Postal Code" />
				</header>
			</Column>
			<Column minScreenWidth="Desktop" demandPopin="true" hAlign="Center">
				<header>
					<Text text="City" />
				</header>
			</Column>
			<Column hAlign="End">
				<header>
					<Text text="Country" />
				</header>
			</Column>
		</columns>
	</TableSelectDialog>
</core:FragmentDefinition>